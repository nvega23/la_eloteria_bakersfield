{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["vite/client", "react", "react-dom"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
